---
export const prerender = false;

// DATA //
import { newsBulletin } from "@/data/media/news-bulletin";

const year: number =
  Number(Astro.url.searchParams.get("year")) || newsBulletin.years[0];

const newsId = Astro.params.newsBulletinId;

// Find that one item dynamically (no static paths)
const newsBulletinItem = newsBulletin.news[year].find(
  (item) => item.id.toString() === newsId,
);
---

{
  newsBulletinItem?.contentUrl && (
    <iframe src={newsBulletinItem.contentUrl} width="100%" height="600" />
  )
}
