---
import EmptyState from "../../components/EmptyState.astro";
import Hero from "../../components/Hero.astro";
import Layout from "../../layouts/Layout.astro";
import Breadcrumbs from "../../components/Breadcrumbs.astro";
---

<Layout>
  <Hero
    title="A Hero Header can be this big and should be bold."
    subtitle="Admissions, school life, or more — we’re here to help"
    ctaText="Scroll to Admission Form"
    variant="withBg"
    imageName="result-image"
  />
  <EmptyState />
  <Breadcrumbs
    items={[
      { label: "Home", href: "/" },
      { label: "Components Library", href: "/components-library" },
    ]}
  />
</Layout>
