---
// COMPONENTS //
import Button from "@/components/ui/Button.astro";
import Image from "astro/components/Image.astro";
import AdmissionHeroImage from "@/assets/images/admission-hero.png";
---

<!-- CRITICAL CSS (Inline) -->
<style is:inline>
  .hero-admission-critical {
    position: relative;
    overflow: hidden;
    background-color: #ffffff;
    background-image: url('/images/school-background.png');
    background-size: cover;
    background-position: center;
    padding-top: 6rem;
  }
  
  @media (min-width: 768px) {
    .hero-admission-critical {
      padding-top: 15rem;
    }
  }
  
  .hero-admission-container {
    position: relative;
    z-index: 10;
    width: 100%;
    margin-right: auto;
    margin-left: auto;
    margin-bottom: 3.75rem;
    padding: 2.5rem 1rem 0;
  }

  @media (min-width: 640px) {
    .hero-admission-container {
      max-width: 640px;
    }
  }

  @media (min-width: 768px) {
    .hero-admission-container {
      max-width: 768px;
      margin-bottom: 7.5rem;
      padding-left: 1.5rem;
      padding-right: 1.5rem;
    }
  }

  @media (min-width: 1024px) {
    .hero-admission-container {
      max-width: 1024px;
      padding-bottom: 5.6875rem;
    }
  }

  @media (min-width: 1280px) {
    .hero-admission-container {
      max-width: 1280px;
      margin-bottom: 0;
    }
  }

  @media (min-width: 1536px) {
    .hero-admission-container {
      
      padding-top: 8rem;
    }
  }

  

  .hero-text-wrapper {
    display: flex;
    flex-direction: column;
    gap: 0.5rem;
  }
  
  @media (min-width: 768px) {
    .hero-text-wrapper {
      gap: 1.75rem; 
      padding-bottom: 5rem;
    }
  }
  
  .hero-title {
    font-size: 2.25rem;
    line-height: 1.25;
    font-weight: 500;
    color: #1e293b;
  }
  
  @media (min-width: 768px) {
    .hero-title {
      font-size: 3.75rem;
      font-weight: 600;
    }
  }
  
  @media (min-width: 1024px) {
    .hero-title {
      font-size: 4.5rem;
    }
  }
  
  .hero-subtitle {
    font-family: "Poppins", sans-serif;
    font-size: 1rem;
    line-height: 1.5;
    font-weight: 500;
    color: #475569;
    width: 100%;
  }
  
  @media (min-width: 1024px) {
    .hero-subtitle {
      width: 45%;
      font-size: 1.25rem;
    }
  }
</style>

<section
  class="hero-admission-critical"
  data-section="Admission Hero"
>
  <!-- Main Content -->
  <div class="hero-admission-container">
    <div class="flex flex-col lg:flex-row lg:items-center lg:gap-0">
      <!-- Left column -->
      <div class="hero-text-wrapper">
        <!-- Hero Text -->
        <div class="w-full md:w-[80%] lg:text-xl xl:w-[55%]">
          <p class="hero-title fade-in-up fade-title">
            Begin Your Child’s Journey
            <span class="text-orange-500"> at SAI </span>
          </p>
        </div>

        <!-- Supporting text -->
        <p class="hero-subtitle fade-in-up fade-subtitle">
          We’re here to guide you every step of the way — from your first
          inquiry to the moment your child walks into our world.
        </p>
      </div>
    </div>
  </div>

  <!-- Inline image (mobile & tablet) -->
  <div
    class="relative flex items-center justify-center lg:absolute lg:-right-50 lg:bottom-0 lg:w-[55%] lg:justify-end"
  >
    <div class="relative">
      <!-- Main Parent image -->

      <div class="fade-in relative w-screen lg:w-full">
        <Image
